   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated callcentre -->
      <editor
            id="uk.ac.york.callcentre.presentation.CallcentreEditorID"
            name="%_UI_CallcentreEditor_label"
            icon="icons/full/obj16/CallcentreModelFile.gif"
