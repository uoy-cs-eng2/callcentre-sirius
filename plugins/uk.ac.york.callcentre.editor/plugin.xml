<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated callcentre -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="uk.ac.york.callcentre.presentation.CallcentreModelWizardID"
            name="%_UI_CallcentreModelWizard_label"
            class="uk.ac.york.callcentre.presentation.CallcentreModelWizard"
